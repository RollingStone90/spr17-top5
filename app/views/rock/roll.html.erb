<div class="row top5">
  <h2> My Top 5 </h2>

  <% count = 1 %>
    <% @songs.each do |song| %>
      <div class="well well-lg">
      	<h1><%= count %></h1>
        <h2><%= song.name %></h2>
				<h3>by <%= song.artist.name %> </h3>
				<p><% "#{song.rating}".to_i.times do |rank| %>
				  <span class="glyphicon glyphicon-star"></span>
        <% end %>
        </p>
        <%= song.video.html_safe %>	             
     </div>
     <% count += 1%>
  <% end %>
</div>


